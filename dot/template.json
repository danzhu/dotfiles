{
  "autostart": ["udiskie", "transmission-daemon"],
  "path": ["$HOME/bin", "$HOME/.local/bin", "$HOME/.config/emacs/bin"],
  "env": {
    "DOT": "$HOME/.dotfiles",
    "EDITOR": "emacs -nw",
    "VISUAL": "emacs -nw",
    "LESS": "R",
    "TERMINAL": "urxvt",
    "ALTERNATE_EDITOR": "",
    "QT_QPA_PLATFORMTHEME": "qt5ct",
    "GTK_IM_MODULE": "fcitx",
    "QT_IM_MODULE": "fcitx",
    "XMODIFIERS": "@im=fcitx",
    "PLT_READLINE_LIB": "/usr/lib/libreadline.so",
    "FZF_DEFAULT_COMMAND": "fd --type f",
    "FZF_DEFAULT_OPTS": "--color=16",
    "XDG_CONFIG_HOME": "$HOME/.config",
    "XDG_CACHE_HOME": "$HOME/.cache",
    "XDG_DATA_HOME": "$HOME/.local/share",
    "CARGO_HOME": "$XDG_DATA_HOME/cargo",
    "GOPATH": "$XDG_DATA_HOME/go",
    "INPUTRC": "$XDG_CONFIG_HOME/readline/inputrc",
    "NPM_CONFIG_USERCONFIG": "$XDG_CONFIG_HOME/npm/npmrc",
    "PASSWORD_STORE_DIR": "$XDG_DATA_HOME/pass",
    "PYLINTHOME": "$XDG_CACHE_HOME/pylint",
    "PYLINTRC": "$XDG_CONFIG_HOME/pylint/pylintrc",
    "PYTHONPYCACHEPREFIX": "$XDG_CACHE_HOME/python",
    "PYTHONUSERBASE": "$XDG_DATA_HOME/python",
    "RUSTUP_HOME": "$XDG_DATA_HOME/rustup",
    "SQLITE_HISTORY": "$XDG_DATA_HOME/sqlite_history",
    "DOTNET_CLI_TELEMETRY_OPTOUT": "true",
    "NEXT_TELEMETRY_DISABLED": "1",
    "GATSBY_TELEMETRY_DISABLED": "1",
    "SAM_CLI_TELEMETRY": "0",
    "DISABLE_VERSION_CHECK": "1",
    "DISABLE_TELEMETRY": "1",
    "DISABLE_ERROR_REPORTING": "1",
    "SONAR_TELEMETRY_ENABLE": "true",
    "NUXT_TELEMETRY_DISABLED": "1",
    "QUILT_DISABLE_USAGE_METRICS": "true",
    "POWERSHELL_TELEMETRY_OPTOUT": "1",
    "DISABLE_ANONYMOUS_TELEMETRY": "true",
    "SFDX_DISABLE_TELEMETRY": "true"
  },
  "alias": {
    "ls": "ls -h --color=auto",
    "la": "ls -A",
    "ll": "ls -l",
    "lal": "ls -Al",
    "mv": "mv -iv",
    "cp": "cp -iv --reflink=auto",
    "ln": "ln -iv",
    "rm": "rm -Iv",
    "df": "df -h",
    "du": "du -h",
    "mkdir": "mkdir -pv",
    "grep": "grep --color=auto",
    "diff": "diff --color=auto",
    "rsync": "rsync --info=progress2",
    "sd": "sudo ",
    "ga": "git add",
    "gaa": "git add --all",
    "gb": "git branch",
    "gc": "git commit",
    "gce": "git commit --allow-empty-message --message \"\"",
    "gca": "git commit --amend --no-edit --allow-empty-message",
    "gd": "git diff",
    "gdc": "git diff --cached",
    "gdu": "git diff @ @{upstream}",
    "gs": "git status",
    "gsl": "git show",
    "gf": "git fetch",
    "gm": "git merge --no-edit",
    "gmu": "git merge --ff-only @{upstream}",
    "gr": "git reset",
    "gl": "git log --graph",
    "glu": "git log --graph @{upstream}",
    "grt": "git rev-parse --show-toplevel",
    "gcl": "git clone",
    "gps": "git push",
    "gpl": "git pull",
    "gco": "git checkout",
    "gcb": "git checkout -b",
    "gst": "git stash save",
    "gsp": "git stash pop",
    "gsd": "git stash drop",
    "+x": "chmod +x",
    "-x": "chmod -x"
  },
  "ssh": {
    "se": {
      "Hostname": "ubuntu1604-006.student.cs.uwaterloo.ca",
      "User": "h94zhu"
    }
  },
  "partition": {
    "root": "/",
    "win": "/win"
  },
  "terminal": "urxvt",
  "terminal_run": "-e {}",
  "window_manager": "i3",
  "editor_gui": "emacs",
  "editor_tui": "emacs -nw",
  "browser_gui": "firefox",
  "name": "Daniel Zhu",
  "email": "danzhu24@hotmail.com",
  "github": "danzhu",
  "dpi": "96",
  "font_mono": "DejaVu Sans Mono",
  "font_size": "9",
  "keymap": "dvorak",
  "xkb_layout": "us",
  "xkb_variant": "dvorak",
  "xkb_options": "ctrl:nocaps",
  "display_primary": "DisplayPort-0",
  "display_secondary": "HDMI-A-0",
  "foreground": "cdd7db",
  "background": "1d1e1f",
  "black": "282b2d",
  "red": "e46868",
  "green": "64d064",
  "yellow": "ddb573",
  "blue": "8488ee",
  "magenta": "b87ee1",
  "cyan": "52bf9d",
  "white": "92989f",
  "light_black": "373a3f",
  "light_red": "f880a3",
  "light_green": "abe46a",
  "light_yellow": "dad66b",
  "light_blue": "90c1fb",
  "light_magenta": "f79dfa",
  "light_cyan": "6fd6e7",
  "light_white": "f0f4f8"
}
