{
  "autostart": ["udiskie", "transmission-daemon", "numlockx"],
  "path": [
    "$HOME/bin",
    "$HOME/.local/bin",
    "$HOME/.config/emacs/bin",
    "$HOME/.local/share/npm/bin"
  ],
  "env": {
    "DOT": "$HOME/.dotfiles",
    "EDITOR": "emacs -nw",
    "VISUAL": "$EDITOR",
    "LESS": "R",
    "TERMINAL": "alacritty",
    "ALTERNATE_EDITOR": "",
    "QT_QPA_PLATFORMTHEME": "qt5ct",
    "GTK_IM_MODULE": "fcitx",
    "QT_IM_MODULE": "fcitx",
    "XMODIFIERS": "@im=fcitx",
    "PLT_READLINE_LIB": "/usr/lib/libreadline.so",
    "FZF_DEFAULT_COMMAND": "fd --type f --strip-cwd-prefix",
    "FZF_DEFAULT_OPTS": "--color=16",
    "FZF_CTRL_T_COMMAND": "$FZF_DEFAULT_COMMAND",
    "FZF_CTRL_T_OPTS": "--preview 'bat --color=always {} --style=\"plain\"'",
    "XDG_CONFIG_HOME": "$HOME/.config",
    "XDG_CACHE_HOME": "$HOME/.cache",
    "XDG_DATA_HOME": "$HOME/.local/share",
    "CARGO_HOME": "$XDG_DATA_HOME/cargo",
    "GHCUP_USE_XDG_DIRS": "1",
    "GOPATH": "$XDG_DATA_HOME/go",
    "INPUTRC": "$XDG_CONFIG_HOME/readline/inputrc",
    "NPM_CONFIG_USERCONFIG": "$XDG_CONFIG_HOME/npm/npmrc",
    "PASSWORD_STORE_DIR": "$XDG_DATA_HOME/pass",
    "PYLINTHOME": "$XDG_CACHE_HOME/pylint",
    "PYLINTRC": "$XDG_CONFIG_HOME/pylint/pylintrc",
    "PYTHONPYCACHEPREFIX": "$XDG_CACHE_HOME/python",
    "PYTHONUSERBASE": "$XDG_DATA_HOME/python",
    "RUSTUP_HOME": "$XDG_DATA_HOME/rustup",
    "SQLITE_HISTORY": "$XDG_DATA_HOME/sqlite_history",
    "STACK_XDG": "1",
    "DOTNET_CLI_TELEMETRY_OPTOUT": "true",
    "NEXT_TELEMETRY_DISABLED": "1",
    "GATSBY_TELEMETRY_DISABLED": "1",
    "SAM_CLI_TELEMETRY": "0",
    "DISABLE_VERSION_CHECK": "1",
    "DISABLE_TELEMETRY": "1",
    "DISABLE_ERROR_REPORTING": "1",
    "SONAR_TELEMETRY_ENABLE": "true",
    "NUXT_TELEMETRY_DISABLED": "1",
    "QUILT_DISABLE_USAGE_METRICS": "true",
    "POWERSHELL_TELEMETRY_OPTOUT": "1",
    "DISABLE_ANONYMOUS_TELEMETRY": "true",
    "SFDX_DISABLE_TELEMETRY": "true"
  },
  "alias": {
    "ls": "ls -h --color=auto",
    "la": "ls -A",
    "ll": "ls -l",
    "lal": "ls -Al",
    "mv": "mv -iv",
    "cp": "cp -iv --reflink=auto",
    "ln": "ln -iv",
    "rm": "rm -Iv",
    "df": "df -h",
    "du": "du -h",
    "mkdir": "mkdir -pv",
    "grep": "grep --color=auto",
    "diff": "diff --color=auto",
    "rsync": "rsync --info=progress2",
    "sd": "sudo ",
    "ga": "git add",
    "gaa": "git add --all",
    "gb": "git branch",
    "gc": "git commit",
    "gce": "git commit --allow-empty-message --message \"\"",
    "gca": "git commit --amend --no-edit --allow-empty-message",
    "gd": "git diff",
    "gdc": "git diff --cached",
    "gdu": "git diff @ @{upstream}",
    "gs": "git status",
    "gsl": "git show",
    "gf": "git fetch",
    "gm": "git merge --no-edit",
    "gmu": "git merge --ff-only @{upstream}",
    "gr": "git reset",
    "gl": "git log --graph",
    "gla": "git log --graph --all",
    "glu": "git log --graph @{upstream}",
    "grt": "git rev-parse --show-toplevel",
    "gcl": "git clone",
    "gps": "git push",
    "gpl": "git pull",
    "gco": "git checkout",
    "gcb": "git checkout -b",
    "gst": "git stash save",
    "gsp": "git stash pop",
    "gsd": "git stash drop",
    "+x": "chmod +x",
    "-x": "chmod -x"
  },
  "partition": {
    "root": "/"
  },
  "terminal": "alacritty",
  "terminal_run": "-e {}",
  "window_manager": "i3",
  "editor_gui": "emacs",
  "editor_tui": "emacs -nw",
  "browser_gui": "firefox",
  "name": "Daniel Zhu",
  "email": "danzhu24@hotmail.com",
  "github": "danzhu",
  "dpi": "96",
  "cursor_size": "24",
  "font_mono": "DejaVu Sans Mono",
  "font_size": "9",
  "keymap": "dvorak",
  "xkb_layout": "us",
  "xkb_variant": "dvorak",
  "xkb_options": "ctrl:nocaps",
  "display_primary": "DisplayPort-0",
  "display_secondary": "HDMI-A-0",
  "foreground": "cdd7db",
  "background": "1d1e1f",
  "black": "282b2d",
  "red": "dc6262",
  "green": "46c348",
  "yellow": "e4ab6b",
  "blue": "928ef3",
  "magenta": "b871f4",
  "cyan": "2aba9d",
  "white": "92989f",
  "light_black": "373a3f",
  "light_red": "fa8bae",
  "light_green": "b2e95b",
  "light_yellow": "f5e979",
  "light_blue": "90baff",
  "light_magenta": "efa4fb",
  "light_cyan": "3fdce1",
  "light_white": "f0f4f8",
  "light_grey": "7b838c"
}
