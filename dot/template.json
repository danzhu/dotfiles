{
    "autostart": [
        "dunst",
        "udiskie",
        "libinput-gestures-setup start",
        "transmission-daemon"
    ],
    "path": ["$HOME/bin", "$HOME/.local/bin"],
    "env": {
        "DOT": "$HOME/.dotfiles",
        "EDITOR": "emacsclient --tty",
        "VISUAL": "emacsclient --tty",
        "LESS": "R",
        "TERMINAL": "termite",
        "ALTERNATE_EDITOR": "",
        "QT_QPA_PLATFORMTHEME": "qt5ct",
        "GTK_IM_MODULE": "fcitx",
        "QT_IM_MODULE": "fcitx",
        "XMODIFIERS": "@im=fcitx",
        "PLT_READLINE_LIB": "/usr/lib/libreadline.so",
        "FZF_DEFAULT_COMMAND": "fd --type f",
        "FZF_DEFAULT_OPTS": "--color=16",
        "DOTNET_CLI_TELEMETRY_OPTOUT": "true"
    },
    "alias": {
        "ls": "ls -h --color=auto",
        "la": "ls -A",
        "ll": "ls -l",
        "lal": "ls -Al",
        "mv": "mv -iv",
        "cp": "cp -iv",
        "ln": "ln -iv",
        "rm": "rm -Iv",
        "df": "df -h",
        "du": "du -h",
        "sd": "sudo ",
        "ga": "git add",
        "gaa": "git add --all",
        "gb": "git branch",
        "gc": "git commit",
        "gce": "git commit --allow-empty-message --message \"\"",
        "gca": "git commit --amend --no-edit --allow-empty-message",
        "gd": "git diff",
        "gdc": "git diff --cached",
        "gdu": "git diff @ @{upstream}",
        "gs": "git status",
        "gf": "git fetch",
        "gm": "git merge --no-edit",
        "gmu": "git merge --ff-only @{upstream}",
        "gr": "git reset",
        "gl": "git log --graph",
        "grt": "git rev-parse --show-toplevel",
        "gcl": "git clone",
        "gps": "git push",
        "gpl": "git pull",
        "gco": "git checkout",
        "gcb": "git checkout -b",
        "gst": "git stash save",
        "gsp": "git stash pop",
        "gsd": "git stash drop",
        "mkdir": "mkdir -pv",
        "grep": "grep --color=auto",
        "diff": "diff --color=auto",
        "+x": "chmod +x",
        "-x": "chmod -x"
    },
    "ssh": {
        "se": {
            "Hostname": "ubuntu1604-006.student.cs.uwaterloo.ca",
            "User": "h94zhu"
        }
    },
    "partition": {
        "root": "/",
        "data": "/mnt/data"
    },
    "terminal": "termite",
    "terminal_run": "-e \"{}\"",
    "window_manager": "i3",
    "launcher": "rofi -show drun -show-icons",
    "runner": "rofi -show run",
    "window_switcher": "rofi -show window -show-icons",
    "pdf_viewer": "zathura",
    "doc_viewer": "zeal",
    "diff_tool": "emerge",
    "editor_gui": "emacsclient --create-frame",
    "editor_tui": "emacsclient --tty",
    "browser_gui": "firefox",
    "browser_tui": "w3m",
    "file_manager_gui": "termite -e ranger",
    "file_manager_tui": "ranger",
    "tty_login": "startx",
    "ssh_login": "tmux new -As ssh",
    "name": "Daniel Zhu",
    "email": "danzhu24@hotmail.com",
    "github": "danzhu",
    "dpi": "192",
    "font_mono": "DejaVu Sans Mono",
    "font_size": "9",
    "keymap": "dvorak",
    "xkb_layout": "us",
    "xkb_variant": "dvorak",
    "xkb_options": "ctrl:nocaps",
    "foreground": "d3d7d8",
    "background": "232425",
    "black": "323539",
    "red": "e66b58",
    "green": "22c75f",
    "yellow": "d8b944",
    "blue": "737fe7",
    "magenta": "a867e2",
    "cyan": "42bca3",
    "white": "7b818a",
    "light_black": "41444b",
    "light_red": "f27f9a",
    "light_green": "88d46b",
    "light_yellow": "dfb971",
    "light_blue": "74b7e7",
    "light_magenta": "e19ae6",
    "light_cyan": "46ded5",
    "light_white": "f0f3f4"
}
