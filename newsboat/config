# config
auto-reload yes
browser {{browser_gui}}
confirm-exit yes
prepopulate-query-feeds yes
reload-threads 4
reload-time 10

# notification
notify-format "%n unread articles"
notify-program "~/.config/newsboat/notify"

# key binds
bind-key j down
bind-key k up

# color
color info color12 black
color listfocus default black
color listfocus_unread default black bold
