# -*- mode: sh -*-

export PATH="{{ for path dir [ }}{{dir}}:{{ ] }}$PATH"

eval "$(dircolors -b)"

%% for env var val [
export {{var}}="{{val}}"
%% ]

if [[ -f ~/.profile ]]; then
    source ~/.profile
fi

# setxkbmap -variant dvorak -option ctrl:nocaps

%% for autostart cmd [
{{cmd}} &
%% ]

